Cell[BoxData[
 RowBox[{
  RowBox[{"vfnfast", "[", 
   RowBox[{
   "A_", ",", "\[Mu]_", ",", "\[Sigma]_", ",", "\[Delta]_", ",", "x_"}], 
   "]"}], ":=", 
  RowBox[{
   RowBox[{"A", "^", "2"}], " ", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"g", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[Delta]", "^", "5"}], "+", 
           RowBox[{"\[Sigma]", "^", "5"}], "+", 
           RowBox[{"2.69296", " ", 
            RowBox[{"\[Sigma]", "^", "4"}], " ", "\[Delta]"}], "+", 
           RowBox[{"2.42843", " ", 
            RowBox[{"\[Sigma]", "^", "3"}], " ", 
            RowBox[{"\[Delta]", "^", "2"}]}], "+", 
           RowBox[{"4.47163", " ", 
            RowBox[{"\[Sigma]", "^", "2"}], " ", 
            RowBox[{"\[Delta]", "^", "3"}]}], "+", 
           RowBox[{"0.07842", " ", "\[Sigma]", " ", 
            RowBox[{"\[Delta]", "^", "4"}]}]}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"1", "/", "5"}], ")"}]}]}], ",", "eta"}], "}"}], ",", "\n", 
     RowBox[{
      RowBox[{"eta", "=", 
       RowBox[{"\[Delta]", "/", "g"}]}], ";", "\n", 
      RowBox[{"eta", "=", 
       RowBox[{"eta", "*", 
        RowBox[{"(", 
         RowBox[{"1.36603", " ", "-", " ", 
          RowBox[{"0.47719", " ", "eta"}], "+", 
          RowBox[{"0.11116", " ", 
           RowBox[{"eta", "^", "2"}]}]}], ")"}]}]}], ";", "\n", 
      RowBox[{
       RowBox[{"eta", "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"x", " ", "-", " ", "\[Mu]"}], ")"}], "^", "2"}], "/", 
             RowBox[{"(", 
              RowBox[{"2", "*", 
               RowBox[{"g", "^", "2"}]}], ")"}]}], ")"}]}], "*", "g", "*", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"2", "*", "Pi"}], "]"}]}], ")"}]}], " ", "+", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", " ", "-", " ", "eta"}], ")"}], "/", 
        RowBox[{"(", 
         RowBox[{"g", "*", "Pi", "*", 
          RowBox[{"(", 
           RowBox[{"1", " ", "+", " ", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"x", " ", "-", " ", "\[Mu]"}], ")"}], "^", "2"}], "/", 
             RowBox[{"g", "^", "2"}]}]}], ")"}]}], ")"}]}]}]}]}], "\n", 
    "]"}]}]}]], "Code"],

