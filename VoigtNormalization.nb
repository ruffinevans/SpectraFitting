Cell["\<\
The slope does depend very slightly on \[Delta]. So the only two nontrivial \
variables are \[Sigma] and \[Delta]. They vary very smoothly, so we can just \
make a lookup table for these.\
\>", "Text",
 CellChangeTimes->{{3.6160716822996364`*^9, 3.6160717463204255`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Sigma]Table", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Sigma]", ",", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{"vfnfast", "[", 
          RowBox[{"1", ",", "0", ",", "\[Sigma]", ",", "\[Delta]", ",", "x"}],
           "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "10"}], ",", "10"}], "}"}]}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Delta]", ",", "0.1", ",", "2.1", ",", "0.5"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Sigma]", ",", "0.001", ",", "10", ",", "0.1"}], "}"}]}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", "\[Sigma]Table", "]"}]}], "Input",
 CellChangeTimes->{{3.616071659612472*^9, 3.616071673876006*^9}}],

